if "Collateral" in word_list:
        automation_results.append(("Collateral Flow","Collateral data present in Snapshot", "Passed"))
        
    else:
        automation_results.append(("Collateral Flow","Collateral data present in Snapshot", "Not Present"))
    
    if "Valuation" in word_list:
        automation_results.append(("Valuation Flow","Valuation data present in Snapshot", "Passed"))
        
    else:
        automation_results.append(("Valuation Flow","Valuation data present in Snapshot", "Not Present"))
    
