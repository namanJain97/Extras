# List of words to check
        words_to_check = ["Ref Date", "Report Date", "Execution Entity Final", "Message Type", "Regulator", "Asset Class Final", "Delegated", "Final Status", "Timeliness", "Count", "Exception Status"]
        
        header_cell_elements = []
        
        # Find all elements with class "ag-header-cell-text"
        header_cell_element = driver.find_elements(By.CLASS_NAME, "ag-header-cell-text")
        
        click_element = driver.find_element(By.XPATH, "//div[contains(@row-id, 'RR3QWICWWIPCS8A4S074.CollateralValue.Dodd-Frank Act (US).NONE.HOUSE.Submitted')]")
        element_inside = click_element.find_element(By.XPATH, ".//div[contains(@col-id, 'messageType')]")
        element_inside.click()
        element_inside.send_keys(Keys.END)
        time.sleep(1)
        
        header_cell_element2 = driver.find_elements(By.CLASS_NAME, "ag-header-cell-text")
        
        header_cell_elements.append(header_cell_element)
        header_cell_elements.append(header_cell_element2)
        print(header_cell_elements)

        # Initialize a set to keep track of words that are not found
        missing_words = set(words_to_check)

        # Iterate through each header cell element
        for element in header_cell_elements:
            text = element.text
            for word in words_to_check:
                if word in text:
                    missing_words.discard(word)

        if missing_words:
            daily_mi_automation_results.append(("Daily MI Fields Check", "Fields are missing for Daily MI", "Failed"))
        else:
            daily_mi_automation_results.append(("Daily MI Fields Check", "All the fields present for Daily MI", "Passed"))


