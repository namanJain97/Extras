package com.rbs.tntr.business.taggingService.configuration;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.context.annotation.Configuration;

/**
 * Created by matbina on 24/06/2020.
 */
@Configuration
public class TaggingServiceQueryConfiguration {
    @Value("${tntr.anvil.append.trade.query}")
    String tradeQuery;

    @Value("${tntr.anvil.append.collateral.query}")
    String collateralQuery;

    @Value("${tntr.anvil.append.valuation.query}")
    String valuationQuery;

    @Value("${tntr.anvil.append.aggregatedcoll.query}")
    String aggregatedcollQuery;

    @Value("${tntr.anvil.append.recon.query}")
    String reconQuery;

    public String getTradeQuery() {
        return tradeQuery;
    }

    public void setTradeQuery(String tradeQuery) {
        this.tradeQuery = tradeQuery;
    }

    public String getCollateralQuery() {
        return collateralQuery;
    }

    public void setCollateralQuery(String collateralQuery) {
        this.collateralQuery = collateralQuery;
    }

    public String getValuationQuery() {
        return valuationQuery;
    }

    public void setValuationQuery(String valuationQuery) {
        this.valuationQuery = valuationQuery;
    }

    public String getAggregatedcollQuery() {
        return aggregatedcollQuery;
    }

    public void setAggregatedcollQuery(String aggregatedcollQuery) {
        this.aggregatedcollQuery = aggregatedcollQuery;
    }

    public String getReconQuery() {
        return reconQuery;
    }

    public void setReconQuery(String reconQuery) {
        this.reconQuery = reconQuery;
    }
}
