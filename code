 private async getAllDetails(serviceUrl: string): Promise<Map<string, ProcessDetails>> {
    try {

      const res = await fetch(
        `${serviceUrl}${getSelectedEnvironmentConfig().endpointFluidProcess}details`,
        {
          method: 'GET',
          headers: {
            'Content-Type': 'application/json'
          }
        }
      );

      if (!res.ok) {
        throw new Error(res.statusText);
      }

      const result: ProcessDetails[] = await res.json();
      const processes = new Map(result.filter(i => i.IsRunning).map(i => [i.Id, i]));
      return processes;

    } catch (e) {
      return new Map();
    }

  }
