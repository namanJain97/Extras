first_row = driver.find_element(By.XPATH, "//div[contains(@class, 'ag-row-odd ag-row-no-focus ag-row ag-row-level-0 ag-row-position-absolute ag-row-not-inline-editing') and contains(@row-index, '1')]")
        first_column = first_row.find_element(By.XPATH, "//div[contains(@class, 'ag-cell ag-cell-not-inline-editing ag-cell-normal-height padding-horizontal-tiny ag-cell-value') and contains(@col-id, 'reportDate')]")
        first_column.click()
        time.sleep(1)
        driver.context_click(first_column)
        time.sleep(1)
        driver.find_element(By.XPATH, "//span[contains(@class, 'ag-menu-option-part ag-menu-option-text') and contains(text(),'Export')]").click()
        time.sleep(1)
        driver.find_element(By.XPATH, "//span[contains(@class, 'ag-menu-option-part ag-menu-option-text') and contains(text(),'CSV Export')]").click()
        time.sleep(1)
