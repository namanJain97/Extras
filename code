def plot_count_dict(countDict):
    categories = list(countDict.keys())
    values = list(countDict.values())
    plt.bar(categories, values)
    plt.xlabel('Features')
    plt.ylabel('Count')
    plt.title('MAS System X : Data structure view')
    plt.xticks(rotation=45)

    for i,v in enumerate(values):
        plt.text(i,v, str(v), ha='center', va='bottom', fontweight = 'bold')
    filename = f'plots/Data structure view_{time.strftime("%Y%m%d_%H%M%S")}.png'
    plt.tight_layout()
    plt.savefig(filename)
    #plt.show()
