import requests

eureka_url = "http://ignite-eureka-uat:21011/eureka/apps/IGNITE.LDN.UAT4.ITR2/"
response = requests.get(eureka_url)

if response.status_code == 200:
    raw_data = response.text
    print("Raw Response Data:")
    print(raw_data)
else:
    print(f"Failed to retrieve data from Eureka. Status code: {response.status_code}")
