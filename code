try:
    if is_service_running(service_url, service_name):
        print("TNTR.Kortex service is running.")
    else:
        # Show a popup message when the service is not running
        root = tk.Tk()
        root.withdraw()
        messagebox.showerror("Service Not Running", "TNTR.Kortex service is not running for UAT.")
        root.destroy()

except Exception as e:
    print(f"Error: {e}")
