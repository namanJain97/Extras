selectJiraTypeAction = "//div[@role='listbox']//div[contains (text(), 'Over Reporting')]"

def select_jiraType(driver):
    sel_jiraType = driver.find_element(By.XPATH, selectJiraType)
    sel_jiraType.click()
    sel_jiraType.send_keys(Keys.ENTER)
    select_jira_type = driver.find_element(By.XPATH, selectJiraTypeAction)
    select_jira_type.click()
    time.sleep(1)
