def update_csvFile():
    
    # Load the CSV file into a DataFrame
    df = pd.read_csv('C:/Users/jainnbr/Downloads/All_Data.csv')
    
    # Define your regex pattern and replacement text
    regex_pattern = r'\bTNTR(.*)\TNTR-\d{4}\b'
    replacement_text = '"$&"'

    # Use the replace method with regex across the entire DataFrame
    df.replace(regex_pattern, replacement_text, regex=True, inplace=True)

    # Save the modified DataFrame back to a CSV file
    df.to_csv('output_file.csv', index=False)
        
    print("csv file updated")


